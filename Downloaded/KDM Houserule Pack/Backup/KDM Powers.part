   <RulesElement name="Disguise Self" type="Power" internal-id="ID_FMP_POWER_1209" source="Player&apos;s Handbook" revision-date="8/27/2010 11:24:17 AM" >
      <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_9,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_10,ID_INTERNAL_CATEGORY_MINOR_ACTION,6 </Category>
