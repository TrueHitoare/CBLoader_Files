   <RulesElement name="Assassin&apos;s Shroud" type="Power" internal-id="ID_FMP_POWER_9400" source="Dragon Magazine 379" revision-date="8/27/2010 11:48:52 AM" >
      <Prereqs> Unselectable </Prereqs>
