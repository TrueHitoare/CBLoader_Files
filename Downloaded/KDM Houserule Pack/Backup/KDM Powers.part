	<RulesElement name="Aimed Shot" type="Power" internal-id="ID_FMP_POWER_13585" source="Player Essentials: Heroes of the Forgotten Kingdoms">
    <Category> ID_DBB_CLASS_FEATURE_100011,ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_AT-WILL_USAGE,ID_FMP_CLASS_788,ID_FMP_CATEGORY_36,ID_INTERNAL_CATEGORY_STANDARD_ACTION </Category>
