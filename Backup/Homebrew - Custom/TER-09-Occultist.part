  <!-- Occultist Class Features -->
	<RulesElement name="Occultist" type="Class" internal-id="TER_CLASS_004" source="Terra Advena" revision-date="8/27/2010 12:40:06 PM">
		<Flavor> The power of the heavens is mine to command - at my discretion, my will all that shapes it. </Flavor>
		<specific name="Key Abilities"> Charisma, Constitution, Intelligence </specific>
		<specific name="Implements"> Ki focuses, staffs, tomes, wands </specific>
		<specific name="Armor Proficiencies"> Cloth </specific>
		<specific name="Weapon Proficiencies"> Dagger, quarterstaff </specific>
		<specific name="Bonus to Defense"> +1 Fortitude, +1 Will </specific>
		<specific name="Hit Points at 1st Level"> 10+ Constitution Score </specific>
		<specific name="Hit Points per Level Gained"> 4 </specific>
		<specific name="Healing Surges"> 6+ Constitution Modifier </specific>
		<specific name="Trained Skills"> Arcana. From the class skills list below, choose 4 more trained skills at 1st level. </specific>
		<specific name="Class Skills"> Arcana (Int), Bluff (Cha), Diplomacy (Cha), Dungeoneering (Wis), Endurance (Con), History (Int), Insight (Wis), Intimidate (Cha), Perception (Wis), Religion (Int) </specific>
		<specific name="Build Options"> Focused Arcanist, Overflowing Vessel </specific>
		<specific name="_PARSED_CLASS_FEATURE"> Covenant Manifestation, Forceful Caster, Occultist Cantrips, Ritual Casting, Warding Invocation </specific>
		<specific name="Role"> Controller. </specific>
		<specific name="Power Source"> Arcane. </specific>
		<specific name="Creating"> Occultists rely on Charisma, Intelligence, and Constitution. No occultist powers or class features depend on Strength, Dexterity, or Wisdom. </specific>
		<specific name="Class Features"> You have the following class features. </specific>
		<specific name="Supplemental" />
		<specific name="Implement"> </specific>
		<specific name="Powers"> </specific>
		<specific name="Power Name"> Spells </specific>
		<specific name="_RoleElement"> ID_FMP_ROLE_1 </specific>
		<specific name="_PowerSourceElement"> ID_FMP_POWER_SOURCE_2 </specific>
		<specific name="_SecondaryPowerSourceElement" />
		<specific name="Secondary Abilities" />
		<specific name="Hybrid Talent Options" />
		<specific name="Short Description"> An invoker variant that uses Charisma and casts as an arcane caster. </specific>
		<specific name="Trait Package" />
		<rules>
			<grant name="ID_INTERNAL_GRANTS_OCCULTIST" type="Grants" />
			<select type="Power" number="2" Category="$$CLASS,at-will,1" />
			<textstring name="_CLASSNAME" value="TER_CLASS_003" />
			<statadd name="_LEVEL-ONE-HPS" value="+10" />
			<statadd name="_PER-LEVEL-HPS" value="+4" />
			<statadd name="Healing Surges" value="+6" />
			<statadd name="Fortitude Defense Class Bonus" value="+1" />
			<statadd name="Will Defense Class Bonus" value="+1" />
			<select type="Skill Training" number="4" Category="TER_CLASS_004" />
		</rules>
	</RulesElement>
