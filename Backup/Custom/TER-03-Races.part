	<RulesElement name="Blood Fury (Alt)" type="Racial Trait" internal-id="ID_TER_RACIAL_TRAIT_0028" source="Terraism" revision-date="10/5/2010 1:47:14 PM" >
		<specific name="Short Description"> +1/2/3 (per tier) to damage when bloodied allies are near. </specific>
		<specific name="Level"> 1 </specific>
		<rules>
			<statadd name="Blood Fury" value="+2" />
			<statadd name="Blood Fury" value="+4" requires="Paragon Tier" />
