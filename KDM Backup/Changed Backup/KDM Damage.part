   <RulesElement name="Offensive Focus" type="Category" internal-id="ID_INTERNAL_CATEGORY_OFFENSIVE_FOCUS" source="KDM" />

   <RulesElement name="Offensive Focus (All)" type="Feat" internal-id="ID_INTERNAL_FEAT_OFFENSIVE_FOCUS_(ALL)" source="KDM" >
   <Category> ID_INTERNAL_CATEGORY_OFFENSIVE_FOCUS </Category>
	<Prereqs>  </Prereqs>
      <specific name="Short Description"> Gain +1 damage per tier. </specific>
      <rules>
         <statadd name="Offensive Focus" value="+1" />
         <statadd name="Offensive Focus" value="+1" requires="Paragon Tier" />
         <statadd name="Offensive Focus" value="+1" requires="Epic Tier" />
         <statadd name="damage rolls" value="+Offensive Focus" type="Feat" />
      </rules>
You gain a +1 per tier feat bonus to damage rolls.
   </RulesElement>

