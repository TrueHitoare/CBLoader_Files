    <specific name="Level"> 11 </specific>
    <specific name="_CS_ShortDescription"> @ </specific>
    <rules>
      <statadd name="Stealth Misc" value="+2" />
      <statadd name="resist:necrotic" value="+10" type="resist" />
	  <statadd name="damage rolls" value="+ABILITYMOD(Dexterity)" condition="while you are insubstantial" />
