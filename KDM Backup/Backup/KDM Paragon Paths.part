   <RulesElement name="Watchman's Barrier" type="Power" internal-id="ID_KDM_POWER_WATCHMAN'S_BARRIER" source="KDM" >
      <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_DAILY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_KDM_PARAGON_PATH_KUNDARAK_WATCHMAN,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_26,ID_FMP_CATEGORY_8,ID_INTERNAL_CATEGORY_STANDARD_ACTION,ID_INTERNAL_CATEGORY_MISS,20 </Category>
