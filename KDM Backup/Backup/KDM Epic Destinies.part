	<RulesElement name="Reality Distortion" type="Power" internal-id="ID_FMP_POWER_6600" source="Dragon Magazine 372" revision-date="8/27/2010 11:42:39 AM" >
      <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_EPIC_DESTINY_297,ID_FMP_CATEGORY_39,ID_INTERNAL_CATEGORY_STANDARD_ACTION,26 </Category>
