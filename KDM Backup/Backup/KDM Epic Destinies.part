   <RulesElement name="Doom&apos;s Reward" type="Class Feature" internal-id="ID_FMP_CLASS_FEATURE_1300" source="Player&apos;s Handbook 2" revision-date="10/5/2010 1:47:04 PM" >
      <Category> 30 </Category>
      <Prereqs> 21st level </Prereqs>
