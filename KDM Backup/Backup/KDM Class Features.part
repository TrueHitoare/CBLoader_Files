   <RulesElement name="Primal Predator" type="Class Feature" internal-id="ID_FMP_CLASS_FEATURE_1122" source="Player&apos;s Handbook 2" revision-date="10/5/2010 1:47:04 PM" >
      <Category> ID_FMP_CLASS_FEATURE_784,1 </Category>
      <specific name="Short Description"> While not wearing heavy armor, gain +1 speed; in beast form, shift as minor action </specific>
      <specific name="Level"> 1 </specific>
