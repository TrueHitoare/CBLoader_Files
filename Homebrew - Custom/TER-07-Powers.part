	<RulesElement name="Voltaic Tracer" type="Power" internal-id="ID_TER_POWER_SOR_001" source="Custom" revision-date="8/27/2010 11:38:32 AM">
		<Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_AT-WILL,ID_INTERNAL_CATEGORY_AT-WILL_USAGE,ID_FMP_CLASS_128,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_25,ID_FMP_CATEGORY_11,ID_INTERNAL_CATEGORY_STANDARD_ACTION,1 </Category>
		<Flavor> A stream of electric current strikes your foe, leaving behind a path of light to assist your further attacks. </Flavor>
		<specific name="Power Usage"> At-Will </specific>
		<specific name="_SkillPower" />
		<specific name="Display"> Sorcerer Attack 1 </specific>
		<specific name="Keywords"> Arcane, Implement, Lightning </specific>
		<specific name="Action Type"> Standard action </specific>
		<specific name="Attack Type"> Ranged 10 </specific>
		<specific name="Target"> One creature </specific>
		<specific name="Attack"> Charisma +1 vs. Reflex </specific>
		<specific name="Hit"> 1d8 + Charisma modifier lightning damage, and you gain a +1 bonus to your next attack roll against the target.
Level 21: 2d8 + Charisma modifier lightning damage. </specific>
		<specific name="Special"> This power can be used as a ranged basic attack. </specific>
		<specific name="_Associated Feats" />
		<specific name="Class"> ID_FMP_CLASS_128 </specific>
		<specific name="Level"> 1 </specific>
		<specific name="Power Type"> Attack </specific>
		<specific name="_BasicAttack"> Ranged </specific>
		<rules>
			<modify name="this" Field="_BasicAttack" value="Ranged" />
		</rules>
	</RulesElement>
