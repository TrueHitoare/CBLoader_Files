			<grant name="ID_FMP_RACIAL_TRAIT_4506" type="Racial Trait" />
			<grant name="ID_INTERNAL_RACIAL_TRAIT_BLUFF_BONUS" type="Racial Trait" />
			<grant name="ID_INTERNAL_RACIAL_TRAIT_STEALTH_BONUS" type="Racial Trait" />
			<grant name="ID_TER_RACIAL_TRAIT_0001" type="Racial Trait" />
		</rules>
	</RulesElement>
	<RulesElement name="Flock Effect" type="Racial Trait" internal-id="ID_FMP_RACIAL_TRAIT_1524" source="Monster Manual 2,Dragon Magazine 411">
