			<grant name="ID_FMP_RACIAL_TRAIT_2327" type="Racial Trait" />
			<grant name="ID_FMP_RACIAL_TRAIT_406" type="Racial Trait" />
			<grant name="ID_FMP_RACIAL_TRAIT_1139" type="Racial Trait" />
