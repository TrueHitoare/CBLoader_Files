<RulesElement name="Kundarak Barrier" type="Power" internal-id="ID_TER_POWER_DRAGONMARK-12-4" source="Terraism">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_FMP_CATEGORY_31,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_INTERNAL_CATEGORY_STANDARD_ACTION,ID_FMP_CLASS_0,10 </Category>
