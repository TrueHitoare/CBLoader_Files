    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_FMP_CATEGORY_31,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_INTERNAL_CATEGORY_MINOR_ACTION,ID_FMP_CLASS_0,6 </Category>
