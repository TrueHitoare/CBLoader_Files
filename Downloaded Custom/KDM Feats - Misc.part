   <RulesElement name="Improved Swordmage Warding" type="Feat" internal-id="ID_FMP_FEAT_1143" source="Arcane Power" revision-date="8/27/2010 12:18:41 PM" >
      <Prereqs> swordmage, Swordmage Warding class feature </Prereqs>
      <print-prereqs> swordmage, Swordmage Warding class feature </print-prereqs>
      <specific name="Tier"> Heroic </specific>
      <specific name="Short Description"> Increase AC bonus from Swordmage Warding </specific>
      <specific name="Special" />
      <specific name="type" />
      <specific name="Associated Power Info" />
      <specific name="Associated Powers" />
      <rules>
         <statadd name="Swordmage Warding" value="+1" type="Improved Swordmage Warding" not-wearing="armor:" />
