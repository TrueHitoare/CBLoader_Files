      <Prereqs> 11th level, swordmage, Swordmage Warding class feature; Paragon Tier </Prereqs>
      <print-prereqs> 11th level, swordmage, Swordmage Warding class feature </print-prereqs>
      <specific name="Tier"> Paragon </specific>
      <specific name="Short Description"> +1 to defenses with Swordmage Warding </specific>
      <specific name="Special" />
      <specific name="type" />
      <specific name="Associated Power Info" />
      <specific name="Associated Powers" />
      <rules>
         <statadd name="AC" value="+1" type="Feat" />
         <statadd name="Fortitude Defense" value="+1" type="Feat" />
         <statadd name="Reflex Defense" value="+1" type="Feat" />
