      <specific name="Short Description"> +2 Athletics; augment at-wills </specific>
      <specific name="Special" />
      <specific name="type"> Lesser Style </specific>
      <specific name="Associated Power Info" />
      <specific name="Associated Powers"> Brash Assault, Marauder&apos;s Rush, Reaping Strike </specific>
      <rules>
         <statadd name="Athletics Misc" value="+2" type="Feat" />
      </rules>
