   <RulesElement name="Summon Couatl" type="Power" internal-id="ID_FMP_POWER_11848" source="Dragon Magazine 385" revision-date="8/27/2010 12:02:02 PM" >
      <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_DAILY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_9,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_25,ID_FMP_CATEGORY_11,ID_FMP_CATEGORY_2,ID_FMP_CATEGORY_51,ID_INTERNAL_CATEGORY_MINOR_ACTION,19 </Category>
