	</RulesElement>
	
  <RulesElement name="Meld into Stone" type="Power" internal-id="ID_FMP_POWER_14508" source="Heroes of the Feywild">
    <Category> ID_INTERNAL_CATEGORY_CLASS_POWER,ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_126,ID_FMP_CATEGORY_33,ID_INTERNAL_CATEGORY_MINOR_ACTION,6 </Category>
