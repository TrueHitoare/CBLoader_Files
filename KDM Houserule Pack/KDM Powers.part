	<RulesElement name="Summon Shadow Serpent" type="Power" internal-id="ID_FMP_POWER_4106" source="Arcane Power" revision-date="8/27/2010 11:33 AM" >
      <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_9,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_25,ID_FMP_CATEGORY_51,ID_INTERNAL_CATEGORY_MINOR_ACTION,2 </Category>
