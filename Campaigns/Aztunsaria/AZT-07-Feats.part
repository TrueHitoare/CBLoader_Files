	<RulesElement name="Implement Expertise (Ki Focuses)" type="Feat" internal-id="ID_INTERNAL_FEAT_IMPLEMENT_EXPERTISE_(KI_FOCUSES)" source="Player's Handbook 2">
		<specific name="Tier"> Heroic </specific>
		<specific name="Short Description"> +1 to implement power attacks with ki focuses </specific>
